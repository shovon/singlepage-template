{
  "scripts": {
    "dev": "concurrent 'npm run dev:webpack' 'npm run dev:livereload' 'npm run dev:watch:jade'",
    "dev:webpack": "webpack-dev-server --content-base ./public --inline",
    "dev:livereload": "livereload public",
    "dev:watch:jade": "chokidar 'src/index.jade' -c 'npm run build:jade'",
    "build": "rm -rf public && NODE_ENV=production webpack && npm run build:jade",
    "build:jade": "node build-html"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "jade": "^1.11.0",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "livereload": "^0.4.1",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.8.0",
    "tsd": "^0.6.5",
    "typescript": "^1.7.5",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {}
}
